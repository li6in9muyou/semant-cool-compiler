#!/usr/bin/env bash

if ! make -j8 semant; then
    exit 1
fi

./test_one -q /class/examples/semant/my-class-cyclic-inheritance.bad.cl
./test_one -q /class/examples/semant/my-class-duplicate.bad.cl
