./test_one -q /class/examples/semant/my-class-no-Main.bad.cl
./test_one -q /class/examples/semant/my-class-simple-Main.cl
./test_one -q /class/examples/semant/my-class-simple-inheritance-tree.cl
./test_one -q /class/examples/semant/my-class-superclass-undefined.bad.cl
./test_one -q /class/examples/semant/my-feature-Main-has-no-main.bad.cl
./test_one -q /class/examples/semant/my-feature-attribute-dependency.bad.cl
./test_one -q /class/examples/semant/my-feature-attribute-type-undefined.bad.cl
./test_one -q /class/examples/semant/my-feature-duplicate-features.bad.cl
./test_one -q /class/examples/semant/my-feature-shadow-attribute-simple.bad.cl
./test_one -q /class/examples/semant/my-feature-shadow-attribute.bad.cl
./test_one -q /class/examples/semant/my-method-formal-forbid-self-type.bad.cl
./test_one -q /class/examples/semant/my-method-formal-type-undefined.bad.cl
./test_one -q /class/examples/semant/my-method-return-type.bad.cl
./test_one -q /class/examples/semant/my-new-simple.cl
./test_one -q /class/examples/semant/my-new-valid-classes.bad.cl
./test_one -q /class/examples/semant/my-arithmetic.bad.cl
./test_one -q /class/examples/semant/my-logics.bad.cl
./test_one -q /class/examples/semant/my-method-return-type.bad.cl
./test_one -q /class/examples/semant/my-object-id.bad.cl
